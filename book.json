{
    "title": "LeetCode",
    "author": "瞿龙俊",
    "description": "记录Gitbook的配置和一些插件的使用",
    "language": "zh-hans",
    "plugins": [
        "-lunr",
        "-search",
        "search-pro",
        "back-to-top-button",
        "chapter-fold",
        "expandable-chapters-small",
        "tbfed-pagefooter",
        "splitter",
        "accordion",
        "prism",
        "prism-themes",
        "-highlight"
    ],
    "pluginsConfig": {
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy 瞿龙俊 2020 - 2021",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "prism": {
            "css": [
              "prism-themes/themes/prism-material-light.css"
            ]
          }
    }
}